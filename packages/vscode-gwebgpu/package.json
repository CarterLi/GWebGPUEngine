{
    "name": "vscode-gwebgpu",
    "displayName": "G for GWebGPU",
    "description": "A language using in compute shader for GWebGPU based on TypeScript",
    "version": "0.6.0",
    "repository": "https://github.com/antvis/GWebGPUEngine.git",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "gwebgpu",
                "aliases": [
                    "g",
                    "gwebgpu"
                ],
                "extensions": [
                    ".g"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gwebgpu",
                "scopeName": "source.g",
                "path": "./syntaxes/gwebgpu.tmLanguage.json"
            }
        ]
    },
    "publisher": "xiaoiver-antfin"
}
