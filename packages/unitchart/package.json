{
  "name": "@antv/g-webgpu-unitchart",
  "version": "0.6.0",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "sideEffects": true,
  "files": [
    "dist",
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "tsc": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist; rimraf es; rimraf lib;",
    "build": "run-p build:*",
    "build:cjs": "BABEL_ENV=cjs babel src --root-mode upward --out-dir lib --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "build:esm": "BABEL_ENV=esm babel src --root-mode upward --out-dir es --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "watch": "BABEL_ENV=esm babel src --watch --root-mode upward --out-dir es --source-maps --extensions .ts,.tsx --delete-dir-on-start"
  },
  "author": "xiaoiver",
  "license": "ISC",
  "dependencies": {
    "@antv/g-webgpu": "^0.6.0",
    "@antv/g-webgpu-compiler": "^0.6.0",
    "@antv/g-webgpu-core": "^0.6.0",
    "@antv/g-webgpu-engine": "^0.6.0",
    "d3": "^5.16.0",
    "gl-matrix": "^3.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/gl-matrix": "^2.4.5",
    "@types/lodash": "^4.14.138"
  },
  "publishConfig": {
    "access": "public"
  }
